{
    "name": "infihex/lanms",
    "description": "LAN-party Managment System",
    "keywords": [],
    "license": "CC-BY-NC-ND-4.0",
    "type": "project",
    "authors": [
        {
            "name": "Daniel S. Billing",
            "email": "daniel@infihex.com",
            "homepage": "https://dsbilling.no",
            "role": "Senior Developer"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "anlutro/l4-settings": "^0.11.0",
        "authy/php": "^3.0",
        "barryvdh/laravel-dompdf": "^0.8.6",
        "binarycabin/laravel-uuid": "^1.0",
        "cartalyst/sentinel": "^3.0",
        "cartalyst/stripe": "~2.0",
        "cartalyst/stripe-laravel": "11.0.*",
        "dialect/laravel-gdpr-compliance": "^1.4",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.4",
        "gerardojbaez/money": "^0.3.1",
        "igaster/laravel-theme": "^2.0",
        "intervention/image": "2.*",
        "intervention/imagecache": "2.*",
        "laravel/framework": "^6.20",
        "laravel/helpers": "^1.3",
        "laravel/passport": "^9.3.2",
        "laravel/tinker": "^2.5",
        "matriphe/larinfo": "^2.0",
        "milon/barcode": "^6.0",
        "nesbot/carbon": "^2.40.0",
        "propaganistas/laravel-phone": "^4.1",
        "rap2hpoutre/laravel-log-viewer": "^1.6.0",
        "sentry/sentry-laravel": "^1.9.0",
        "spatie/laravel-activitylog": "^3.2.2",
        "spatie/laravel-searchable": "^1.9",
        "spatie/laravel-sluggable": "^2.2",
        "symfony/translation": "~4.1",
        "twilio/sdk": "^5.31"
    },
    "require-dev": {
        "facade/ignition": "^1.16.4",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "LANMS\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
