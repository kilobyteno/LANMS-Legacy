{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "anlutro/l4-settings": "^0.11.0",
        "authy/php": "^3.0",
        "barryvdh/laravel-dompdf": "^0.8.6",
        "binarycabin/laravel-uuid": "^1.0",
        "cartalyst/sentinel": "3.*",
        "cartalyst/stripe": "~2.0",
        "cartalyst/stripe-laravel": "11.0.*",
        "dialect/laravel-gdpr-compliance": "^1.4",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "gerardojbaez/money": "^0.3.1",
        "igaster/laravel-theme": "^2.0",
        "intervention/image": "2.*",
        "intervention/imagecache": "2.*",
        "laravel/framework": "^6.0",
        "laravel/helpers": "^1.3",
        "laravel/passport": "^9.3.2",
        "laravel/tinker": "^1.0",
        "matriphe/larinfo": "^2.0",
        "milon/barcode": "^6.0",
        "nesbot/carbon": "^2.40.0",
        "propaganistas/laravel-phone": "^4.1",
        "rap2hpoutre/laravel-log-viewer": "^1.6.0",
        "sentry/sentry-laravel": "^1.3.0",
        "spatie/laravel-activitylog": "^3.2.2",
        "spatie/laravel-sluggable": "^2.2",
        "symfony/translation": "~4.1",
        "twilio/sdk": "^5.31"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "classmap": [
            "database",
            "database/factories"
        ],
        "psr-4": {
            "LANMS\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": ",tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
     "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
